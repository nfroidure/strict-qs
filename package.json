{
  "name": "strict-qs",
  "version": "1.0.1",
  "description": "A stricter Query String parser",
  "main": "src/index.js",
  "metapak": {
    "data": {
      "files": "src/*.js",
      "testsFiles": "src/*.mocha.js"
    }
  },
  "scripts": {
    "cli": "env NODE_ENV=${NODE_ENV:-cli}",
    "cover": "istanbul cover _mocha --report html -- src/*.mocha.js -R spec -t 5000",
    "coveralls": "istanbul cover _mocha --report lcovonly -- src/*.mocha.js -R spec -t 5000 && cat ./coverage/lcov.info | coveralls && rm -rf ./coverage",
    "doc": "jsdoc2md src/index.js > API.md",
    "lint": "eslint src/*.js",
    "metapak": "metapak || echo 'Please `npm install --save-dev metapak`' exit 0",
    "postinstall": "npm run metapak",
    "preversion": "npm t && npm run lint",
    "test": "mocha src/*.mocha.js"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/nfroidure/strict-qs.git"
  },
  "keywords": [
    "qs",
    "query",
    "string",
    "parser"
  ],
  "author": "Nicolas Froidure",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nfroidure/strict-qs/issues"
  },
  "homepage": "https://github.com/nfroidure/strict-qs#readme",
  "devDependencies": {
    "coveralls": "2.11.15",
    "eslint": "3.4.0",
    "eslint-config-simplifield": "4.1.1",
    "istanbul": "0.4.5",
    "jsdoc-to-markdown": "2.0.1",
    "metapak": "0.0.13",
    "metapak-nfroidure": "0.0.9",
    "mocha": "3.2.0",
    "mocha-lcov-reporter": "1.2.0"
  },
  "dependencies": {
    "debug": "1.0.0",
    "yerror": "^1.0.2"
  },
  "engines": {
    "node": ">=6.9.5"
  }
}
